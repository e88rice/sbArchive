<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.sbarchive.mapper.SignBoardMapper">

    <insert id="register">
        INSERT INTO signboard (xOffSet, yOffSet, name, address)
        VALUES (#{xOffSet}, #{yOffSet}, #{name}, #{address})
    </insert>

    <select id="getList" resultType="com.project.sbarchive.vo.SignBoardVO">
        select * from signboard;
    </select>

</mapper>